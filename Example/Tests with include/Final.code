// Returns the square root of integer x
int i_sqrt(int n)
{
  int b = 0;

  while(n >= 0)
  {
		n = n - b;
		b = b + 1;
		n = n - b;
  }

  return b - 1;
}
// Returns the power of integer x
int power(int x, int power){
	
	int i=2, result = x;

	while (power >= i){
		result = result * x;
		i = i + 1;
	}

	return result;
}

//returns the absolute value
int abs(int a){
	if (a>0){
		return a;
	} else {
		return 0-a;
	}
}





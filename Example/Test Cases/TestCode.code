real[] reverse(real a[], int length){

	int i;
	
	for (i=0; i<length/2; i++){
		a[i], a[length-i-1] = a[length-i-1], a[i];
	}

	return a;
}

int show(real a[], int length){

	int i;
	
	for (i=0; i<length; i++){
		print a[i];
	}
	
	return 0;
	
}

